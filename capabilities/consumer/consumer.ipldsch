# Schema for the consumer capabilities

# Caveats for the consumer/has capability
type HasCaveats struct {
  consumer String
}

# Success response for the consumer/has capability (boolean)
type HasOk struct {
  has Bool
}

# Caveats for the consumer/get capability
type GetCaveats struct {
  consumer String
}

# Success response for the consumer/get capability
type GetOk struct {
  consumer String
  provider String
}
